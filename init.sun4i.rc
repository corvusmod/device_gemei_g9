	insmod /system/vendor/modules/ft5x_ts.ko
	insmod /system/vendor/modules/goodix_touch.ko
	insmod /system/vendor/modules/ssd253x-ts.ko
#	insmod /system/vendor/modules/inet_ha2608.ko
