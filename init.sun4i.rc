#	insmod /system/vendor/modules/goodix_touch_3F.ko	
#	insmod /system/vendor/modules/ssd253x-ts_3F.ko
	
	insmod /system/vendor/modules/sun4i-keyboard.ko

# insmod mali driver 
	insmod /system/vendor/modules/ump.ko
	insmod /system/vendor/modules/mali.ko

#Rocky@20120104 insmod gsensor driver
	insmod /system/vendor/modules/bma250.ko
#	insmod /system/vendor/modules/bma222.ko
#	insmod /system/vendor/modules/mma7660.ko


#csi module
	insmod /system/vendor/modules/videobuf-core.ko
	insmod /system/vendor/modules/videobuf-dma-contig.ko
